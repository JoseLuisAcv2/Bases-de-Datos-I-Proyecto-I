		\cline{3-4}
		&& Modelo & Nombre y descripción del modelo.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		\hline
		
		
		
		ENTIDAD & SEMANTICA. &  Atributo & Semántica.\\ \cline{3-4}
		% Atributos
		&& Atributo & Semántica.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		\hline
			
		ENTIDAD & SEMANTICA. &  Atributo & Semántica.\\ \cline{3-4}
		% Atributos
		&& Atributo & Semántica.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		&& Atributo & Semántica.\\ \cline{3-4}
		\hline
		
	
	\end{tabular}
